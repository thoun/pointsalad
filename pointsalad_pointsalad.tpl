{OVERALL_GAME_HEADER}

<link rel="stylesheet" href="https://use.typekit.net/jim0ypy.css">

<div id="full-table">
    <div id="columns">
        <div id="main-column">
            <div id="currentplayertable"></div>


            <div id="table-inner">
                <div id="table">
                    <div id="market-title">{MARKET}</div>
                    <div id="piles">
                        <div id="pile1" class="table-space">
                            <div id="pile1-counter" class="pile-counter"></div>
                        </div>
                        <div id="pile2" class="table-space">
                            <div id="pile2-counter" class="pile-counter"></div>
                        </div>
                        <div id="pile3" class="table-space">
                            <div id="pile3-counter" class="pile-counter"></div>
                        </div>
                    </div>
                    <div id="market">
                        <div id="market-row1" class="market-row">
                            <div id="market-row1-card1" class="table-space"></div>
                            <div id="market-row1-card2" class="table-space"></div>
                            <div id="market-row1-card3" class="table-space"></div>
                        </div>
                        <div id="market-row2" class="market-row">
                            <div id="market-row2-card1" class="table-space"></div>
                            <div id="market-row2-card2" class="table-space"></div>
                            <div id="market-row2-card3" class="table-space"></div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="playerstables"></div>
        </div>
        <div id="table-right"></div>
    </div>
    
    <div id="jump-controls">
    </div>
</div>

{OVERALL_GAME_FOOTER}
